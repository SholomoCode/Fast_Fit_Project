@{
}
@model List<FastFitProject.Models.Search>

<h1>All Event Categories</h1>

@if (Model.Count == 0)
{
    <p>No members Categories yet!</p>
}
else
{
    <table class="table">
        <tr>
            <th>Id
            <th>member Name</th>
        </tr>
        @foreach (Search members in Model)
        {
            <tr>
                <td>@members.Id</td>
                <td>@members.Name</td>
            </tr>
        }
    </table>
}